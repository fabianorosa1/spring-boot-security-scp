{
    "welcomeFile": "index.html",
    "routes": [{
        "source": "^/ads/api/",
        "target": "/api/",
        "destination": "backend_api",
        "scope": "$XSAPPNAME.Display",
        "csrfProtection": false
    }, {
        "source": "^/ads",
        "target": "/",
        "destination": "backend_api"
    }, {
        "source": "^/nodeapi/(.*)$",
        "target": "$1",
        "destination": "nodeapi_api"
    }]
}
